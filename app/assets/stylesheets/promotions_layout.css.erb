.per-bkg .form-group p input[type="radio"]:not(:checked) + label:before,
.per-bkg .form-group p input[type="radio"]:checked + label:before{
	top: 17px;
	height: 24px;
}
.per-bkg .form-group p input[type="radio"]:not(:checked) + label:after,
.per-bkg .form-group p input[type="radio"]:checked + label:after{
	top: 20px;
	font-size: 24px;
}
.form-group p input[type="radio"]:not(:checked),
.form-group p input[type="radio"]:checked {
  display: none;
}
.form-group p input[type="radio"]:not(:checked) + label,
.form-group p input[type="radio"]:checked + label {
  position: relative;
  padding-left: 40px;
  cursor: pointer;
  font-size: 21px;
}
.form-group p input[type="radio"]:not(:checked) + label:before,
.form-group p input[type="radio"]:checked + label:before {
  content: '';
  position: absolute;
  left:0;
  top: -5px;
  width:33px;
  height: 30px;
  border: 1px solid #aaa;
  background: #f8f8f8;
  border-radius: 3px;
}
.form-group p  input[type="radio"]:not(:checked) + label:after,
.form-group p  input[type="radio"]:checked + label:after {
  content: 'âœ”';
  position: absolute;
  top: 0;
  left: 6px;
  font-size: 28px;
  line-height: 0.8;
  color: #666666;
  transition: all .2s;
}
.form-group p [type="radio"]:not(:checked) + label:after {
  opacity: 0;
  -moz-transform:scale(0);
  -webkit-transform: scale(0);
  -ms-transform: scale(0);
   transform: scale(0);
}
.form-group p  [type="radio"]:checked + label:after {
  opacity: 1;
  -moz-transform:scale(1);
  -webkit-transform: scale(1);
  -ms-transform: scale(1);
   transform: scale(1);
}
