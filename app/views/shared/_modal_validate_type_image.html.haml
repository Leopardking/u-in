#error-message-image-type.modal.fade{ role: "dialog"}
  .modal-dialog.modal-sm
    .modal-content
      .modal-body
        %div.container-modal
          %p.content-message.font-style
            = t(".please_try_again")
        %div.button-close-modal
          %button.btn.btn-default{type:"button", "data-dismiss"=>"modal"}
            = t("shared.button_ok")
